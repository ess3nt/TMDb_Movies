{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "rules": {
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "comma-dangle": ["error", "never"],
    "no-console": 0,
    "react/jsx-indent-props": ["error", 4],
    "react/jsx-indent": ["error", 4],
    "react/jsx-boolean-value": 0
  }
}